[Unit]
Description=Rclone: WebGUI

[Service]
Type=simple
User=__APP__
Group=__APP__
WorkingDirectory=__INSTALL_DIR__/
ExecStart=__INSTALL_DIR__/rclone rcd --rc-web-gui-no-open-browser --rc-addr=127.0.0.1:__PORT__ --rc-user __ADMIN__ --rc-pass __PASSWORD__ --rc-web-gui-update

[Install]
WantedBy=default.target
